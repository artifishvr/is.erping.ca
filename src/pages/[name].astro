---
import Layout from "../layouts/Layout.astro";
import Search from "../components/Search.svelte";

export const prerender = false;

const { name } = Astro.params;

const title = `is ${name} erping?????`;
---

<Layout title={title}>
    <main>
        <div
            class="flex flex-col justify-center items-center font-semibold min-h-screen text-center"
        >
            <p class="text-4xl">{name} is probably erping</p>

            <Search client:load />
        </div>
    </main>
</Layout>
